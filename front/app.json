{
	"expo": {
		"name": "pawminder",
		"slug": "pawminder",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/logo.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,

		"android": {
			"package": "com.kamuisoulsan.pawminder",
			"versionCode": 1,
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/logo.png",
				"backgroundColor": "#ffffff"
			},
			"permissions": [],
			"softwareKeyboardLayoutMode": "resize"
		},

		"ios": {
			"supportsTablet": true
		},

		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/logo.png"
		},

		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/logo.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			]
		],

		"experiments": {
			"typedRoutes": true
		},

		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "533afc03-e804-4a05-a34c-3904ab10d90a"
			}
		},

		"runtimeVersion": {
			"policy": "sdkVersion"
		}
	}
}
